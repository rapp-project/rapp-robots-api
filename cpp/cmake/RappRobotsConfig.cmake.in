# - Config file for the Rapp package
# It defines the following variables
#  RAPP_ROBOTS_INCLUDE_DIRS     - include directories for Rapp API
#  RAPP_ROBOTS_LIBRARIES        - shared libraries to link against
#  RAPP_ROBOTS_LIBRARIES_STATIC - static libraries to link against
 
# Include directories
set(RAPP_ROBOTS_INCLUDE_DIRS "@CMAKE_INSTALL_PREFIX@/include")
 
# Libraries
set(RAPP_ROBOTS_LIBRARIES @RAPP_SHARED_LIBS@)

set(RAPP_ROBOTS_LIBRARIES_STATIC @RAPP_STATIC_LIBS@)

set(RAPP_ROBOTS_LIBRARY_DIRS "@CMAKE_INSTALL_PREFIX@/lib")

